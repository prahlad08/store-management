<%- include("partials/header") %>
<section id="reports">
    <h1 style="text-align: center; color: black;">Reports</h1>
    <form style="margin: 5%;" action="/reports" method="POST">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="from">From:</label>
                <input type="date" class="form-control" name="from" placeholder="From">
            </div>
            <div class="form-group col-md-6">
                <label for="to">To:</label>
                <input type="date" class="form-control" name="to" placeholder="To">
            </div>
        </div>
        <button type="submit" class="btn btn-block btn-lg btn-outline-primary angled-gradient-button">Confirm</button>
    </form>
</section> 
<%- include("partials/footer") %> 